{
	"$schema": "https://biomejs.dev/schemas/1.6.0/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": false,
		"rules": {
			"suspicious": {
				"noExplicitAny": "off"
			},
			"correctness": {
				"noUnusedImports": "error"
			}
		}
	},
	"formatter": {
		"enabled": false
	}
}