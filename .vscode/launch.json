{
  "version": "1.0.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Jest: current file",
      "program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
      "args": ["${fileBasenameNoExtension}", "--config", "apps/farm-service/jest.config.ts"],
      "console": "integratedTerminal",
      "disableOptimisticBPs": true
    }
  ]
}
